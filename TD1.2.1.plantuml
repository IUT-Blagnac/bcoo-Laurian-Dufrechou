
@startuml

class Examen
{
  date
  coefficient
}

class Enseignant
{
  nom
  prenom
}

class etudiant
{
  idnumEtu
  nom
  prenom
  date_de_naissance
}

class cour
{
  code
  titre
}

etudiant "1.*" --- "0.*" Examen
(etudiant, Examen) . note

etudiant "1.*" --- "1.*" cour : suivre

Examen "1" --- "1" cour : conserner

cour "1.*" --- "1" Enseignant : enseigner

class note
{
  valeur
}

@enduml
